// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model GrizouContestEntry {
  id Int @default(autoincrement()) @id
  firstName String @map(name: "first_name")
  lastName String @map(name: "last_name")
  phone String @unique
  email String @unique
  experience String @db.VarChar(3000)
  projectTitle String @map(name: "project_title") @db.VarChar(3000)
  projectDescription String @map(name: "project_description") @db.VarChar(3000)
}